---
import Layout from "./Layout.astro";
import Page from "src/shared/astro/page.astro";
import PageSVG from "src/components/svg/page.astro";
// import Section from "src/components/layout/section.astro";

type Props = {
  frontmatter: {
    content_blocks: any;
  };
};

const { frontmatter, frontmatter: {content_blocks} } = Astro.props;

if (!frontmatter) {
  throw new Error("Page frontmatter must be defined");
}

---
<Layout {...frontmatter}>
  <PageSVG />
  <Page bookshop:live contentBlocks={content_blocks} />
</Layout>